{
  "name": "hoantn/socialsuite",
  "type": "project",
  "description": "SocialSuite â€” Facebook Page management & scheduling on Laravel 12",
  "keywords": ["laravel", "facebook", "scheduling", "socialite"],
  "license": "proprietary",
  "require": {
    "php": "^8.2",
    "laravel/framework": "^12.0",
	"laravel/socialite": "6.x-dev as 6.0.0",
    "laravel/tinker": "^2.9",
    "guzzlehttp/guzzle": "^7.9",
    "laravel/socialite": "6.x-dev",
    "socialiteproviders/facebook": "^5.15",
    "ext-json": "*"
  },
  "require-dev": {
    "spatie/laravel-ignition": "^2.4",
    "fakerphp/faker": "^1.23",
    "phpunit/phpunit": "^10.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ]
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
