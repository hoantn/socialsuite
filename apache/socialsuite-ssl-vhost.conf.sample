<VirtualHost *:443>
    ServerName socialsuite.local
    DocumentRoot "D:/xampp/htdocs/socialsuite/public"

    SSLEngine on
    SSLCertificateFile "D:/xampp/apache/ssl/localhost.crt"
    SSLCertificateKeyFile "D:/xampp/apache/ssl/localhost.key"

    <Directory "D:/xampp/htdocs/socialsuite/public">
        Options Indexes FollowSymLinks
        AllowOverride All
        Require all granted
    </Directory>

    ErrorLog "D:/xampp/apache/logs/socialsuite-ssl-error.log"
    CustomLog "D:/xampp/apache/logs/socialsuite-ssl-access.log" combined
</VirtualHost>
