# httpd-ssl.conf SAMPLE for Laravel socialsuite on XAMPP (HTTPS)
Listen 443

<VirtualHost _default_:443>
    ServerName localhost:443
    DocumentRoot "D:/xampp/htdocs/socialsuite/public"

    SSLEngine on
    SSLCertificateFile "D:/xampp/apache/ssl/localhost.crt"
    SSLCertificateKeyFile "D:/xampp/apache/ssl/localhost.key"

    <Directory "D:/xampp/htdocs/socialsuite/public">
        Options Indexes FollowSymLinks
        AllowOverride All
        Require all granted
    </Directory>

    ErrorLog "D:/xampp/apache/logs/ssl_error.log"
    CustomLog "D:/xampp/apache/logs/ssl_access.log" combined
</VirtualHost>
